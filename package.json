    "eslint": "^7.3.1",
    "eslint-config-google": "^0.14.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "ts-node-dev": "^1.0.0-pre.49",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  }
}
